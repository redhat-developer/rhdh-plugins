  "files": [
    "dist",
    "dist-scalprum",
    "app-config.yaml",
    "config.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/redhat-developer/rhdh-plugins",
    "directory": "workspaces/bulk-import/plugins/bulk-import"
