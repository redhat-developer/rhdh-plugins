{
  "name": "custom-authentication-provider-module",
  "version": "0.0.1",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/redhat-developer/rhdh-plugins",
    "directory": "workspaces/orchestrator/plugins/custom-authentication-provider-module"
  },
  "backstage": {
    "role": "frontend-plugin",
    "pluginId": "custom-authentication-provider-module",
    "pluginPackages": [
      "custom-authentication-provider-module",
      "custom-authentication-provider-module-backend"
    ]
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "package.json": [
        "package.json"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "tsc": "tsc",
    "build": "backstage-cli package build",
    "prettier:check": "prettier --ignore-unknown --check .",
    "prettier:fix": "prettier --ignore-unknown --write .",
    "lint:check": "backstage-cli package lint",
    "lint:fix": "backstage-cli package lint --fix",
    "test": "backstage-cli package test --passWithNoTests --coverage",
    "clean": "backstage-cli package clean",
    "clean-dynamic-sources": "yarn clean && rm -Rf node_modules",
    "export-dynamic": "janus-cli package export-dynamic-plugin",
    "export-dynamic:clean": "run export-dynamic --clean"
  },
  "dependencies": {
    "@backstage/core-app-api": "^1.19.1",
    "@backstage/core-components": "^0.18.2",
    "@backstage/core-plugin-api": "^1.11.1",
    "@backstage/integration-react": "^1.2.11",
    "@mui/material": "^5.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@backstage/cli": "^0.34.4",
    "@janus-idp/cli": "3.6.1",
    "@types/react": "^18.0.0",
    "prettier": "3.6.2",
    "react-router": "^6.4.5",
    "react-router-dom": "^6.4.5",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0 || ^18.0.0"
  },
  "files": [
    "dist",
    "dist-dynamic/*.*",
    "dist-dynamic/dist/**"
  ]
}
