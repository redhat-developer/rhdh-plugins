{
  "name": "@red-hat-developer-hub/backstage-plugin-lightspeed",
  "version": "1.0.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "backstage": {
    "role": "frontend-plugin",
    "supported-versions": "1.35.1",
    "pluginId": "lightspeed",
    "pluginPackage": "@red-hat-developer-hub/backstage-plugin-lightspeed",
    "pluginPackages": [
      "@red-hat-developer-hub/backstage-plugin-lightspeed",
      "@red-hat-developer-hub/backstage-plugin-lightspeed-backend",
      "@red-hat-developer-hub/backstage-plugin-lightspeed-common"
    ]
  },
  "sideEffects": [
    "./**/*.css"
  ],
  "scripts": {
    "build": "backstage-cli package build",
    "clean": "backstage-cli package clean",
    "lint:check": "backstage-cli package lint",
    "lint:fix": "backstage-cli package lint --fix",
    "postpack": "backstage-cli package postpack",
    "prepack": "backstage-cli package prepack",
    "start": "backstage-cli package start",
    "test": "backstage-cli package test --passWithNoTests --coverage",
    "tsc": "tsc",
    "prettier:check": "prettier --ignore-unknown --check .",
    "prettier:fix": "prettier --ignore-unknown --write ."
  },
  "dependencies": {
    "@backstage/core-components": "^0.17.5",
    "@backstage/core-plugin-api": "^1.10.9",
    "@backstage/plugin-permission-react": "^0.4.36",
    "@backstage/theme": "^0.6.8",
    "@material-ui/core": "^4.9.13",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@mui/icons-material": "^6.1.8",
    "@patternfly/chatbot": "6.4.0-prerelease.26",
    "@patternfly/react-core": "6.4.0-prerelease.3",
    "@patternfly/react-icons": "^6.3.1",
    "@red-hat-developer-hub/backstage-plugin-lightspeed-common": "workspace:^",
    "@tanstack/react-query": "^5.59.15",
    "react-markdown": "^9.0.1",
    "react-use": "^17.2.4"
  },
  "peerDependencies": {
    "react": "16.13.1 || ^17.0.0 || ^18.0.0",
    "react-router-dom": "^6.0.0"
  },
  "devDependencies": {
    "@backstage/cli": "^0.34.1",
    "@backstage/core-app-api": "^1.18.0",
    "@backstage/dev-utils": "^1.1.13",
    "@backstage/test-utils": "^1.7.11",
    "@emotion/is-prop-valid": "^1.3.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@spotify/prettier-config": "^15.0.0",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "14.6.1",
    "msw": "1.3.5",
    "prettier": "3.6.2",
    "react": "16.13.1 || ^17.0.0 || ^18.0.0",
    "react-dom": "16.13.1 || ^17.0.0 || ^18.0.0",
    "react-router-dom": "^6.0.0"
  },
  "files": [
    "dist",
    "config.d.ts",
    "dist-scalprum",
    "app-config.yaml"
  ],
  "scalprum": {
    "name": "red-hat-developer-hub.backstage-plugin-lightspeed",
    "exposedModules": {
      "Alpha": "./src/alpha.ts",
      "PluginRoot": "./src/index.ts",
      "LightspeedPlugin": "./src/index.ts"
    }
  },
  "configSchema": "config.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redhat-developer/rhdh-plugins.git",
    "directory": "workspaces/lightspeed/plugins/lightspeed"
  },
  "keywords": [
    "lifecycle:active",
    "backstage",
    "plugin"
  ],
  "homepage": "https://red.ht/rhdh",
  "bugs": "https://github.com/redhat-developer/rhdh-plugins/issues",
  "maintainers": [
    "@karthikjeeyar",
    "@rohitkrai03"
  ],
  "author": "Red Hat"
}
